
<!DOCTYPE html>
<html ng-app="ratemyself">
<head>
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.5.0/angular.min.js"></script>
</head>
<body>

    <script>
    angular.module('ratemyself', [])
        .controller('testController', function ($scope) {
            $scope.login = function() {
                $scope.t = "test views"
            };
        });
    </script>

    <div class="container">
        <div class="col-sm-8 col-sm-offset-2" ng-controller="testController">
            <h3>Login</h3>
            <form>
                <div class="form-group">
                    <input type="email" class="form-control" placeholder="Email" name="email" ng-model="user.email">
                </div>
                <div class="form-group">
                    <input type="password" class="form-control" placeholder="Password" name="password" ng-model="user.password">
                </div>
                <button type="submit" class="btn btn-default" ng-click="login()">Login</button>
            </form>
        </div>
    </div>
</body>
</html>
